{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "container": {
    "name": "container",
    "connector": "loopback-component-storage",
    "provider": "filesystem",
    "root": "./server/storage"
  },
  "Picuni": {
    "host": "localhost",
    "port": 3306,
    "url": "mysql://root:root@localhost:3306/Picuni",
    "database": "Picuni",
    "password": "root",
    "name": "Picuni",
    "user": "root",
    "connector": "mysql"
  },
  "PicuniMongo": {
    "host": "localhost",
    "port": 27017,
    "url": "mongodb://localhost:27017/PicuniMongo",
    "database": "PicuniMongo",
    "password": "",
    "name": "PicuniMongo",
    "user": "",
    "connector": "mongodb"
  },
  "PicuniRedis": {
    "host": "localhost",
    "port": 6379,
    "url": "redis://localhost:6379/PicuniRedis",
    "password": "",
    "name": "PicuniRedis",
    "connector": "redis",
    "user": ""
  },
  "PicuniES": {
    "name": "PicuniES",
    "connector": "es",
    "index": "postcaption",
    "hosts": [
      {"host": "localhost", "port": 9200}
    ],
    "apiVersion": "2.3",
    "log": "trace",
    "defaultSize": 50,
    "requestTimeout": 30000,
    "mappings": [
      {
        "name": "PostCaption",
        "properties": {
          "postCaptionId": {
            "type": "string",
            "index": "not_analyzed"
          },
          "content": {
            "type": "string",
            "index": "not_analyzed"
          },
          "postId": {
            "type": "string",
            "index": "not_analyzed"
          }
        }
      }
    ]
  }
}

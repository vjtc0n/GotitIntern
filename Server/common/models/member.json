{
  "name": "Member",
  "base": "User",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "userId": {
      "type": "number",
      "id": true,
      "generated": false,
      "required": false,
      "mysql": {
        "columnName": "userId",
        "dataType": "BIGINT",
        "dataLength": 20
      }
    },
    "avatar_picture": {
      "type": "string"
    },
    "realm": false,
    "emailVerified": false,
    "verificationToken": false
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "accessToken",
      "foreignKey": "userId"
    },
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    },
    "posts": {
      "type": "hasMany",
      "model": "Post",
      "foreignKey": "userId"
    },
    "comments": {
      "type": "hasMany",
      "model": "Comment",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "exists"
    }
  ],
  "methods": {}
}
